pipeline {
    environment {
        registry = 'yanishub1/test'
        registryCredential = 'test'
        dockerImage = ''
    }
    agent any
    stages {
        stage('SCM') {
            checkout scm
        }
        stage('SonarQube Analysis') {
            def scannerHome = tool 'test_sonar';
            withSonarQubeEnv() {
                sh "${scannerHome}/bin/sonar-scanner"
            }
        }
    }
}
